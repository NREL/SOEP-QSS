#!/bin/bash
# GCC C++ Build Release

if [ -f $1.cc ]; then
  CxxMainSource=$1.cc
  OutputSpec="-o $1"
  shift
fi

g++ -pipe -std=c++11 -pedantic -Wall -Wextra -Winline -Wno-unknown-pragmas -m64 -march=native -DQSS_FMU -DNDEBUG -Ofast -fno-stack-protector -finline-limit=2000 -s $CxxMainSource $@ $OutputSpec
