#!/bin/bash
# GCC C++ Build Release

if [ -f $1.cc ]; then
  CxxMainSource=$1.cc
  OutputSpec="-o $1"
  shift
fi

g++ -pipe -std=c++11 -pedantic -Wall -Wextra -Winline -Wno-unknown-pragmas -ffor-scope -m64 -march=native -DNDEBUG -Ofast -fno-stack-protector -fcilkplus -Wno-maybe-uninitialized -s $CxxMainSource $@ $OutputSpec
