#!/bin/bash
# QSS Linux Setup

if [ -z "$QSS" ]; then
  export QSS=$(dirname $(dirname $(dirname $(readlink -m "${BASH_SOURCE[0]}"))))
fi

export PATH=$PATH:$QSS/bin/Linux:$QSS/bin

if [ -n "$QSS_bin" ]; then
  if [ -z "$PYTHONPATH" ]; then
    export PYTHONPATH=$QSS_bin
  else
    export PYTHONPATH=$PYTHONPATH:$QSS_bin
  fi
fi
